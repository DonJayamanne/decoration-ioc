{
	"rules": {
		"indent": [
			true,
			4,
			"spaces"
		],
		"quotemark": [
			true,
			"double"
		],
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"curly": true,
		"class-name": true,
		"semicolon": [true],
		"triple-equals": true,
		"no-trailing-whitespace": true,
		"prefer-const": true
	}
}